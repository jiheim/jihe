	border:1px solid #ddd;
	padding:5px;
	background:#fff;
	text-indent:0;
	}

.slide_gallery ul li ul li a:hover{
	border:2px solid #3d3d3d !important;
	border:1px solid #3d3d3d;
}


.slide_gallery ul li.gallery_back a{
	background:url('../images/bt_back_gallary.gif') center no-repeat;
}

.slide_gallery ul li.gallery_next a{
	background:url('../images/bt_next_gallary.gif') center no-repeat;
}


.slide_gallery ul li.gallery_back a:hover{
	background: #e1f5f9 url('../images/bt_back_gallary.gif') center no-repeat;
}

.slide_gallery ul li.gallery_next a:hover{
	background: #e1f5f9 url('../images/bt_next_gallary.gif') center no-repeat;
}


