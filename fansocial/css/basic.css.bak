	width:840px;
	float:left;
	}

.slide_gallery ul li.gallery_next{
	float:right;
	width:20px;
}

.slide_gallery ul li a{
	display:block;
	height:100px;
	width:20px;
	text-indent:-9999px;
}


.slide_gallery ul li ul{
	margin:0;
}


.slide_gallery ul li ul li{
	width:150px;
	height:105px;
