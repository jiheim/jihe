	}

.slide_gallery h4 strong{
	color:#1c7d8e;
	margin-left:10px;
	font-size:12px;
	}


.slide_gallery button{
	float:right;
	display:block;
	width:121px;
	height:32px;
	background:url('../images/bt_upload_photo.gif') 0 0 no-repeat;
	border:0 none;
	text-indent:-9999px;
	cursor:pointer;
	}

.slide_gallery button:hover{
	background-position:0 -33px;
	}

.slide_gallery button:active{
	background-position:0 -66px;
	}


.slide_gallery ul{
 overflow:hidden;
 margin-top:20px;
}

.slide_gallery ul li{
	height:110px;

}

